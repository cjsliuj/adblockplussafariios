# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'AdblockPlusSafari' do
  use_frameworks!
 	pod 'FavIcon', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/FavIcon.git', :tag => '2.0.2'
 	pod 'RxSwift', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/RxSwift.git', :tag => '4.1.2'
 	pod 'RxSwiftExt', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/RxSwiftExt.git', :tag => '3.2.0'
 	#pod 'yajl-dynamic', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/yajl-dynamic', :tag => '2.0.2'
    pod 'RxCocoa', '~> 4.0'
end

target 'AdblockPlusSafariActionExtension' do
   pod 'FavIcon', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/FavIcon.git', :tag => '2.0.2'
  use_frameworks!
 
end

target 'AdblockPlusSafariExtension' do
 
  use_frameworks!

  
end
 
target 'libadblockplus-ios' do
 
  use_frameworks!
	pod 'FavIcon', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/FavIcon.git', :tag => '2.0.2'
	pod 'RxSwift', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/RxSwift.git', :tag => '4.1.2'
	pod 'RxSwiftExt', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/RxSwiftExt.git', :tag => '3.2.0'
	#pod 'yajl-dynamic', :git => 'https://gitlab.com/eyeo/adblockplus/ios-libraries/yajl-dynamic', :tag => '2.0.2'
	pod 'RxCocoa', '~> 4.0'
  
end
